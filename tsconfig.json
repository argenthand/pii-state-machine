{
  "compilerOptions": {
    "declaration": true,
    "lib": ["es6", "dom"],
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false
  },
  "include": [
    "src/**/*"
  ]
}
